package math;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Math.pow(2,3) = " + Math.pow(2, 3));
		System.out.println("Math.max(2, Math.min(3,4)) = " + Math.max(2, Math.min(3, 4)));
		System.out.println("Math.round(2.5F) = " + Math.round(2.5F));
		System.out.println("Math.ceil(-9.49) = " + Math.ceil(-9.49));
		System.out.println("Math.floor(7.5) = " + Math.floor(7.5));
		String s = "test";
	
	}
	

}
